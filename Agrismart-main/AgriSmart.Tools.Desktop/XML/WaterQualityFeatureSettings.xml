  <Features>
    <Feature name="pH" index="1" formatString ="n2" unit="" visible="true" categoryName="PhysicalFeatures" methodName="" featureName="pH" 
             rule="iif(val &#60; 5,'pH ácido',iif(val &#60; 6.5,'pH ligeramente ácido', iif(val &#60;7.5,'pH neutro',iif(val &#60; 8,'pH alcalino','pH muy alcalino'))))">
      <caption>
        <en>pH</en>
        <es>pH</es>
      </caption>
      <display_name>
        <en>Acidity</en>
        <es>Acidez</es>
      </display_name>
    </Feature>
    <Feature name="EC" index="2" formatString ="n2" unit="mS·cm-1" visible="true" categoryName="PhysicalFeatures" methodName="" featureName="EC"
         rule="iif(val=0, 'Sin especificar',iif(val &#60; 0.75, 'Salinidad: Baja', iif(val &#60;= 1.5, 'Salinidad: Moderada', iif(val &#60;= 3, 'Salinidad: Alta', 'Salinidad: Muy alta'))))">
      <caption>
        <en>Electrical Conductity (CE25)</en>
        <es>Conductividad eléctrica (CE25)</es>
      </caption>
      <display_name>
        <en>Electrical Conductity (CE25)</en>
        <es>Conductividad eléctrica (CE25)</es>
      </display_name>
    </Feature>
    <Feature name="HCO3" index="3" formatString ="n2" unit="meq·L-1" visible="true" categoryName="Anions" methodName="" featureName="HCO3"
     rule="iif(val=0,'',iif(val &#60; 3, 'Bajo', iif(val&#60;= 6,'Medio','Alto')))">
